	private static boolean getNextPerm(char[] perm){
		int i;
		for(i = perm.length-1; i > 0;){
			if(perm[i] <= perm[i-1]){
				i--;
			}
			else
				break;
		}
		if(i > 0){
			i--;
		}
		else
			return false;
	}
	

