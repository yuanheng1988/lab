package dp;

public class Solution {
    public int solution(int K, int L, int M, int N, int P, int Q, int R, int S) {
        // write your code here...
    	long centre1X = (K + M)/2;
    	long centre1Y = (L + N)/2;
    	long centre2X = (P + R)/2;
    	long centre2Y = (Q + S)/2;
    	long length1X = M - K;
    	long length1Y = N - L;
    	long length2X = R - P;
    	long length2Y = S - Q;
    	long d1 = (length1X+length2X)/2 - Math.abs(centre1X-centre2X);
    	long d2 = (length1Y+length2Y)/2) - Math.abs(centre1Y-centre2Y);
    	long sum = length1X*length1Y + length2X*length2Y;
    	
    	if(d1 > 0 && d2 > 0){
    		return 
    	}
    	else
    		return length1X*length1Y + length2X*length2Y;
    } 
}
